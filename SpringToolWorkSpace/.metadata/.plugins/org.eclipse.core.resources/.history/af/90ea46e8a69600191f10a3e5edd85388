import java.util.Scanner;

public class Exercise14 {
    public static int countElement(int a[], int n, int i){
		int count= 0;
		for(int j=0 ; j<n ; j++){
			if(a[j]== i)
			count ++;
		}
		return (count);
	}
    public static int EnterValue(){
		Scanner input= new Scanner(System.in);
		boolean check= false;
		int n=0;
		while(check==false){
			System.out.print(" ");
			try{
				n= input.nextInt();
				if (n>0) {
				check= true;
				}
				else {
					System.out.println("Please enter correct number (0)");
					System.out.print("Enter value: ");
				}
			}catch(Exception e){
				input.nextLine();
			}
		}
		return (n);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		  int n,i;
			System.out.println("EnterValue n= ");
			n= EnterValue();
			int[] array= new int[n];
			for(i=0 ; i<n ; i++){
				System.out.println("EnterValue element " +(i+1)+" ");
				array[i]= EnterValue();
			}
			System.out.print("Total number exist 1 time: ");
			for(i=0 ; i<n ; i++){
				if(countElement(array, n, array[i])==1)
	                            System.out.print("  "+array[i]);
	    }
	   }
	}

}
